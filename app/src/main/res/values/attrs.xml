<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="WFYTitle">
        <attr name="centerText" format="reference|string"/>
        <attr name="centerImage" format="reference"/>
        <attr name="centerTextColor" format="reference|color"/>
        <attr name="centerTextSize" format="dimension"/>
        <attr name="leftText" format="reference|string"/>
        <attr name="leftTextColor" format="reference|color"/>
        <attr name="leftImage" format="reference"/>
        <attr name="rightText" format="reference|string"/>
        <attr name="rightTextColor" format="reference|color"/>
        <attr name="rightImage" format="reference"/>
        <attr name="leftIsBack" format="boolean"/>
    </declare-styleable>


  <declare-styleable name="RoundCornerProgress">
    <attr name="rcReverse" format="boolean" />
    <attr name="rcProgress" format="float" />
    <attr name="rcBackgroundPadding" format="dimension" />
    <attr name="rcBackgroundRadius" format="dimension" />
    <attr name="rcMax" format="float" />
    <attr name="rcIconSrc" format="integer" />
    <attr name="rcIconSize" format="dimension" />
    <attr name="rcIconPadding" format="dimension" />
    <attr name="rcIconPaddingRight" format="dimension" />
    <attr name="rcIconPaddingLeft" format="dimension" />
    <attr name="rcIconPaddingTop" format="dimension" />
    <attr name="rcIconPaddingBottom" format="dimension" />
    <attr name="rcHeaderColor" format="color" />
    <attr name="rcProgressColor" format="color" />
    <attr name="rcBackgroundColor" format="color" />
    <attr name="rcTextProgressColor" format="color" />
    <attr name="rcTextProgress" format="string" />
    <attr name="rcTextProgressUnit" format="string" />
    <attr name="rcTextProgressSize" format="dimension" />
    <attr name="rcTextProgressWidth" format="dimension" />
    <attr name="rcTextProgressPadding" format="dimension" />
    <attr name="rcAutoTextChange" format="boolean" />
    <attr name="rcSecondaryProgress" format="float" />
    <attr name="rcSecondaryProgressColor" format="color" />
  </declare-styleable>

  <declare-styleable name="CircleButton">
    <attr name="cb_color" format="color" />
    <attr name="cb_pressedRingWidth" format="dimension" />
  </declare-styleable>



    <declare-styleable name="MaterialProgressBarSupport">
        <attr name="progressSmallSize"
            format="boolean"/>
        <attr name="progressColor"
            format="color"/>
        <attr name="progressBackgroundColor"
            format="color"/>
    </declare-styleable>

    <declare-styleable name="WaterWaveProgress">
        <attr name="progressWidth" format="dimension" />
        <attr name="fontSize" format="dimension" />
        <attr name="progressColors" format="color" />
        <attr name="progressBgColor" format="color" />
        <attr name="waterWaveColor" format="color" />
        <attr name="waterWaveBgColor" format="color" />
        <attr name="progress2WaterWidth" format="dimension" />
        <attr name="showProgress" format="boolean" />
        <attr name="showNumerical" format="boolean" />
        <attr name="textColor" format="color" />
        <attr name="progress" format="integer" />
        <attr name="maxProgress" format="integer" />
    </declare-styleable>

    <declare-styleable name="CirclePageIndicator">
        <!-- Whether or not the indicators should be centered. -->
        <attr name="centered" format="boolean"/>
        <!-- Color of the filled circle that represents the current page. -->
        <attr name="fillColor" format="color" />
        <!-- Color of the filled circles that represents pages. -->
        <attr name="pageColor" format="color" />
        <!-- Orientation of the indicator. -->
        <attr name="android:orientation"/>
        <!-- Radius of the circles. This is also the spacing between circles. -->
        <attr name="radius" format="dimension" />
        <!-- Whether or not the selected indicator snaps to the circles. -->
        <attr name="snap" format="boolean" />
        <!-- Color of the open circles. -->
        <attr name="strokeColor" format="color" />
        <!-- Width of the stroke used to draw the circles. -->
        <attr name="strokeWidth" format="float"/>
        <!-- View background -->
        <attr name="android:background"/>
    </declare-styleable>

    <declare-styleable name="ViewPagerIndicator">
        <attr name="vpiCirclePageIndicatorStyle" format="reference"/>
    </declare-styleable>
</resources>